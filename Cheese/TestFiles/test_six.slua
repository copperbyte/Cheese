--Super Simple Test Six


function while_loop()
  i = 1
  while i <= 10 do
    print(i)
    i = i + 1
  end
end

----

function repeat_loop()
  i = 10
  repeat
    print(i)
    i = i - 1
  until i == 0
end

----

function for_num_loop()
  for i = 1, 100, 8 do
    print(i)
  end  
end

----



while_loop();
repeat_loop();
for_num_loop();
