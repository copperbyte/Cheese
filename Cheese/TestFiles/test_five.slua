function if_else_if() 
  n = 12
  if n > 15 then
    print("the number is > 15")
  elseif n > 10 then
    print("the number is > 10") 
  elseif n > 5 then
    print("the number is > 5")
  else
    print("the number is <= 5")
  end
end

